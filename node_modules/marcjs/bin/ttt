#!/usr/bin/env node

var marc = require('../lib/marcjs');
var fs = require("fs");

var marc_output = new marc.Iso2709Writer(fs.createWriteStream("album_0.mrc"));
var record = new marc.Record;
record.append([ '998', '  ', 'a', 'Demians', 'b', 'Frédéric']);
console.log(record.as('text'));
marc_output.write(record);
marc_output.end();

